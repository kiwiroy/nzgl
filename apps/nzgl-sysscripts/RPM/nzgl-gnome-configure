export GCONF_CONFIG_SOURCE=`gconftool-2 --get-default-source`
gconftool-2 --direct --config-source=$GCONF_CONFIG_SOURCE --recursive-unset /apps/panel > /dev/null || :
gconftool-2 --direct --config-source=$GCONF_CONFIG_SOURCE --recursive-unset /schemas/apps/panel > /dev/null || :
gconftool-2 --direct --config-source=$GCONF_CONFIG_SOURCE --load /etc/gconf/schemas/panel-default-setup.entries.nzgl 
gconftool-2 --direct --config-source=$GCONF_CONFIG_SOURCE --load /etc/gconf/schemas/panel-default-setup.entries.nzgl /apps/panel
